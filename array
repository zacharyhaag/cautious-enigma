#include <iostream>
using namespace std;


void ReverseArray(int array[], int start, int end)

wihile (start < end)
{
  int temp = array[start];
  array[start] = array[end];
  array[end] = temp;
  start++;
  end--;
}

void printArray(int array[], int size)
{
  for (int i = 0; i < size; i++)
  {
    cout << array[i] << " ";
    cout << endl;
  }
}

int main()
{
  int array[] = {1, 2, 3, 4};
  printArray(array, 4);
  ReverseArray(array, 0, 3);
  cout << "Revese array is: "

  printArray(array, 4);

  return 0; 
}
